<Application
    x:Class="DePostelein.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:DePosteleinManagement"
    xmlns:converters="using:DePosteleinManagement.Converter"
    RequestedTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <converters:DoNothingConverter x:Key="myDoNothingConverter"></converters:DoNothingConverter>
            <converters:DateConverter x:Key="myDateConverter"></converters:DateConverter>
            <converters:RoleConverter x:Key="myRoleConverter"></converters:RoleConverter>
        </ResourceDictionary>
    </Application.Resources>
</Application>

